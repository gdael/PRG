    <elementParameter field="MEMO_EDITOR_JAVA" name="CODE" value="//Get process details from globalMap (with null checks)&#xD;&#xA;Long processId = (Long) globalMap.get(&quot;process_id&quot;);&#xD;&#xA;String processName = (String) globalMap.get(&quot;process_nm&quot;);&#xD;&#xA;String db_host = (String) globalMap.get(&quot;db_host&quot;) ;&#xD;&#xA;&#xD;&#xA;log.error(&quot;Error Connecting Process id: &quot; + processId +&quot; | Database Host: &quot;+db_host);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;//Construct email subject&#xD;&#xA;String email_subject = &quot;CONNECTION FAILURE | &quot; + processId + &quot; | &quot; + processName;&#xD;&#xA;&#xD;&#xA;//Print out details for debugging (optional for production)&#xD;&#xA;&#xD;&#xA;System.out.println(&quot;**************************************************************************************************&quot;);&#xD;&#xA;System.out.println(&quot;EMAIL TO: &quot; + (String) globalMap.get(&quot;email&quot;));&#xD;&#xA;System.out.println(&quot;EMAIL FROM: &quot; + context.smtp_from_email);&#xD;&#xA;System.out.println(&quot;SUBJECT: &quot; + email_subject);&#xD;&#xA;&#xD;&#xA;//Build email body (with null safety checks for context variables)&#xD;&#xA;String emailBody = &quot;&lt;p>Hello,&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Here's a list of your detailed event(s):&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Task '&quot; + (context.flow_name != null ? context.flow_name : &quot;Unknown Task&quot;) + &quot;' failed on &quot; + &#xD;&#xA;              (context.flow_start_datetime != null ? context.flow_start_datetime : &quot;Unknown Date&quot;) + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>&lt;b>Summary:&lt;/b>&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Message: Connection Failed&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>&lt;b>Run details:&lt;/b>&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Task Run Id: &quot; + (context.flow_execution_id != null ? context.flow_execution_id : &quot;Unknown ID&quot;) + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Task Id: &quot; + (context.flow_id != null ? context.flow_id : &quot;Unknown ID&quot;) + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Task Name: &quot; + (context.flow_name != null ? context.flow_name : &quot;Unknown Task Name&quot;) + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Workspace Name: &quot; + (context.flow_workspace != null ? context.flow_workspace : &quot;Unknown Workspace&quot;) + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>&lt;b>Audit details:&lt;/b>&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Process Id: &quot; + processId + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>Process Name: &quot; + processName + &quot;&lt;/p>&quot; +&#xD;&#xA;              &quot;&lt;p>&lt;/p>&quot; ;&#xD;&#xA;&#xD;&#xA;System.out.println(&quot;BODY: &quot; + emailBody);&#xD;&#xA;&#xD;&#xA;//SMTP configuration output (optional for debugging)&#xD;&#xA;System.out.println(&quot;EMAIL HOST: &quot; + context.smtp_host_email);&#xD;&#xA;System.out.println(&quot;EMAIL PORT: &quot; + context.smtp_port_email);&#xD;&#xA;System.out.println(&quot;EMAIL USERNAME: &quot; + context.smtp_username_email);&#xD;&#xA;System.out.println(&quot;EMAIL PASSWORD: &quot; + context.smtp_password_email);&#xD;&#xA;System.out.println(&quot;**************************************************************************************************&quot;);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;log.info(&quot;**************************************************************************************************&quot;);&#xD;&#xA;log.info(&quot;EMAIL TO: &quot; + (String) globalMap.get(&quot;email&quot;));&#xD;&#xA;log.info(&quot;EMAIL FROM: &quot; + context.smtp_from_email);&#xD;&#xA;log.info(&quot;SUBJECT: &quot; + email_subject);&#xD;&#xA;&#xD;&#xA;log.info(&quot;BODY: &quot; + emailBody);&#xD;&#xA;&#xD;&#xA;log.info(&quot;EMAIL HOST: &quot; + context.smtp_host_email);&#xD;&#xA;log.info(&quot;EMAIL PORT: &quot; + context.smtp_port_email);&#xD;&#xA;log.info(&quot;EMAIL USERNAME: &quot; + context.smtp_username_email);&#xD;&#xA;log.info(&quot;EMAIL PASSWORD: &quot; + context.smtp_password_email);&#xD;&#xA;log.info(&quot;**************************************************************************************************&quot;);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;globalMap.put(&quot;email_subject&quot;, email_subject);&#xD;&#xA;globalMap.put(&quot;emailBody&quot;, emailBody);&#xD;&#xA;"/>
